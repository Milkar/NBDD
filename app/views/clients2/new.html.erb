<%= render 'user_profile' %>
<h3>Create a Client:</h3>

<% form_for([@user,@client]) do |f| %>
  <%= f.error_messages %>

 
  <div class="field">
    <%= f.label :company %><br />
    <%= f.text_field :company %>
  </div>
  <div class="field">
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </div>
  <div class="field">
    <%= f.label :tel %><br />
    <%= f.text_field :tel %>
  </div>
  <div class="field">
    <%= f.submit 'Create' %>
  </div>
<% end %>
	<div class="linkto_button round">
    <%= link_to 'Back', user_clients_path%>
  </div>
