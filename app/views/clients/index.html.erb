
<%= render 'user_profile' %>


<table>
  <tr>
    
    <th>Company</th>
    <th>Address</th>
    <th>Tel</th>
  </tr>

<% @clients.each do |client| %>

  <tr>
    
    <td><%=h client.company %></td>
    <td><%=h client.address %></td>
    <td><%=h client.tel %></td>
    <td class ="linkto_button round"><%= link_to 'Show', [ @user, client]  %></td>
    <td class ="linkto_button round"><%= link_to 'Edit', [:edit, @user, client] %></td>
    <td class ="linkto_button round"><%= link_to 'Destroy', [@user, client] , :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<div class ="linkto_button round"><%= link_to 'New client', new_user_client_path %></div>